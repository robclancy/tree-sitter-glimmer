{
  "name": "tree-sitter-glimmer",
  "version": "1.2.0",
  "main": "bindings/node",
  "license": "MIT",
  "scripts": {
    "generate": "tree-sitter generate",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test",
    "prepare": "husky install"
  },
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "lint-staged": "^12.4.3",
    "prettier": "^2.8.8",
    "tree-sitter-cli": "^0.20.8"
  },
  "tree-sitter": [
    {
      "scope": "source.glimmer",
      "file-types": [
        "hbs"
      ],
      "injection-regex": "hbs",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "volta": {
    "node": "14.16.0",
    "yarn": "1.22.10"
  },
  "lint-staged": {
    "*.js": "prettier --write"
  }
}
